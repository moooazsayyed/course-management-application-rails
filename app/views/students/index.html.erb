<h1>Students</h1>
<%= link_to 'New Student', new_student_path %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Course</th>
      <th>Department</th>
      <th>Division</th>
      <th>Year</th>
      <th>Email</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.name %></td>
        <td><%= student.course %></td>
        <td><%= student.dept %></td>
        <td><%= student.div %></td>
        <td><%= student.year %></td>
        <td><%= student.email %></td>
        <td><%= link_to 'Show', student_path(student) %></td>
        <td><%= link_to 'Edit', edit_student_path(student) %></td>
        <td><%= link_to 'Delete', destroy_student_path(student), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
